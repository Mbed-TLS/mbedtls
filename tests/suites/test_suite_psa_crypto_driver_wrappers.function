                                input->x, input->len,
                                output, output_buffer_size,
                                &function_output_length );
    TEST_EQUAL( status, PSA_ERROR_BAD_STATE );
    TEST_EQUAL( mbedtls_test_driver_cipher_hooks.hits, 0 );
    psa_cipher_abort( &operation );

